{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "ES2016"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "outDir": "dist"
  },
  "include": [
      "./src/**/*"
  ]
}
